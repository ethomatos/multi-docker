{
	"AWSEBDockerrunVersion": 2,
	"containerDefinitions": [
		{
			"name": "client",
			"image": "ethomatos/multi-client",
			"hostname": "client",
			"essential": false
		},
		{
			"name": "server",
			"image": "ethomatos/multi-server",
			"hostname": "api",
			"essential": false
		},
		{
			"name": "worker",
			"image": "ethomatos/multi-worker",
			"hostname": "worker",
			"essential": false
		},
		{
			"name": "nginx",
			"image": "ethomatos/multi-nginx",
			"hostname": "nginx",
			"essential": true,
			"portMappings": [
				{
					"hostPort": 80,
					"containerPort": 80
				}
			],
			"links": [
				"client", "server"
			]
		}

	]
}
